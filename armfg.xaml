<Window x:Class="Exercise_plan.armfg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exercise_plan"
        mc:Ignorable="d"
        Title="armfg" Height="450" Width="800">
    <Grid>
        <Label Content="Arms 12 x 3 Reps" HorizontalAlignment="Left" Margin="301,10,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" FontStyle="Italic"/>
        <Image Margin="462,234,33,10" Source="bicep-hammer-curls-490x490.jpg" Stretch="Fill"/>
        <Image Margin="73,239,408,10" Source="biceps-b-curls-490x299.jpg" Stretch="Fill"/>
        <Image Margin="457,62,149,214" Source="dumbbell-kickbacks.gif" Stretch="Fill" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.255"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Image Margin="111,57,494,175" Source="rope-bicep-curls.gif" Stretch="Fill"/>
        <Button Content="Back" x:Name="backbtn" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="backbtn_Click"/>

    </Grid>
</Window>
