<Window x:Class="Exercise_plan.shldfm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exercise_plan"
        mc:Ignorable="d"
        Title="shldfm" Height="450" Width="800">
    <Grid>
        <Label Content="Shoulders 12 x 3 Reps" HorizontalAlignment="Left" Margin="301,10,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" FontStyle="Italic"/>
        <Image Margin="459,93,10,157" Source="dumbbell-front-shoulder-raises-490x330.jpg" Stretch="Fill"/>
        <Image Margin="10,93,496,157" Source="shoulders-bent-over-side-raises-490x278.jpg" Stretch="Fill"/>
        <Image Margin="278,212,280,0" Source="shoulder-seated-d-press-490x381.jpg" Stretch="Fill" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.938"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Button Content="Back" x:Name="backbtn" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="75" Click="backbtn_Click" Margin="0,0,0,399"/>

    </Grid>
</Window>
